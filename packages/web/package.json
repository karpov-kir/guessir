{
  "name": "@guessir/web",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "npm run clean && tsc -p ./tsconfig.build.json && vite build",
    "clean": "del-cli ./dist",
    "start:dev": "vite",
    "start:preview": "vite preview",
